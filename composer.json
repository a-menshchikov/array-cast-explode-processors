{
    "name": "nbgrp/env-bundle",
    "type": "symfony-bundle",
    "description": "The bundle provides a few useful custom Symfony Environment Variable processors",
    "keywords": [
        "Symfony",
        "bundle",
        "dependency-injection",
        "environment-variable-processor",
        "env-var-processor"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Alexander Menshchikov",
            "email": "amenshchikov@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1 || ^8.0",
        "ext-filter": "*",
        "symfony/config": "^4.4",
        "symfony/dependency-injection": "^4.4",
        "symfony/http-kernel": "^4.4"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "symfony/phpunit-bridge": "^4.4"
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Nbgrp\\EnvBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nbgrp\\Tests\\EnvBundle\\": "tests/"
        }
    },
    "scripts": {
        "nba": "docker run -it --rm -v \"$(pwd)\":\"/app\" --workdir /app nbgrp/auditor:latest"
    }
}
